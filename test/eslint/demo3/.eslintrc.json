{
    "rules": {
        "no-console": 2,
        "no-debugger": 2,
        "no-delete-var": 2,
        "no-mixed-spaces-and-tabs": [2, true],
        "indent": [2, "tab"],
        "no-dupe-keys": 2,
        "no-dupe-args": 2,
        "no-duplicate-case": 2,
        "no-else-return": 2,
        "no-eval": 2,
        "no-ex-assign": 2,
        "no-floating-decimal": 2,
        "no-func-assign": 2,
        "no-invalid-this": 2,
        "no-redeclare": 2,
        "no-shadow": 2,
        "no-shadow-restricted-names": 2,
        "no-spaced-func": 2,
        "no-sparse-arrays": 2,
        "no-undef": 2,
        "no-undef-init": 2,
        "no-unreachable": 2,
        "no-unused-expressions": 2,
        "no-unused-vars": [2, {
            "vars": "all",
            "args": "after-used"
        }],
        "no-use-before-define": 2,
        "array-bracket-spacing": [2, "never"],
        "brace-style": [2, "1tbs"],
        "camelcase": 2,
        "comma-dangle": [2, "never"],
        "comma-style": [2, "last"],
        "default-case": 2,
        "eqeqeq": 2,
        "func-style": [2, "declaration"],
        "key-spacing": [2, {
            "beforeColon": false,
            "afterColon": true
        }],
        "new-parens": 2,
        "operator-linebreak": [2, "after"],
        "quotes": [1, "double"],
        "id-match": 2,
        "semi": [2, "always"],
        "semi-spacing": [2, {
            "before": false,
            "after": true
        }],
        "space-in-parens": [2, "never"],
        "spaced-comment": 2,
        "strict": 2,
        "use-isnan": 2,
        "vars-on-top": 2
    }
}
