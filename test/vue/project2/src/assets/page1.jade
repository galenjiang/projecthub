doctype
html
  head
    meta(charset="utf-8")
    meta(http-equiv="Content-Type" content="text/html" charset="utf-8")
    meta(name="format-detection" content="telephone=no")
    meta(name="viewport" content="width=device-width,initial-scale=1.0,user-scalable=0,minimum-scale=1.0,maximum-scale=1.0,minimal-ui")
    meta(name="apple-mobile-web-app-status-bar-style" content="black")
    title vue
    script(type="text/javascript").
      document.documentElement.style.fontSize = document.documentElement.clientWidth*100/750*2 + 'px';
    link(rel="stylesheet" href="../css/core/reset_h5.css" media="screen" title="no title" charset="utf-8")
    link(rel="stylesheet" href="../css/index.css" media="screen" title="no title" charset="utf-8")
  body
    .wrapper
      header 
        span(id="ex") a={{ a }}, b={{ b }}
      .content body
        div(id="example")
          template(id="child-template")
            input(v-model="msg")
            button(v-on:click="notify") Dispatch Event

            div(id="events-example")
            p Messages: {{ messages | json }}
            child
      
      footer tail
    
    script(type="text/javascript" src="../js/page1.js")
    
