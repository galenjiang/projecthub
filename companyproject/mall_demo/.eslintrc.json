{
    "extends": "eslint:recommended",
    "globals": {
        "Vue": true,
        "Mui": true,
        "_": true,
        "util": true,
        "Swipe": true,
        "wechat": true,
        "wx": true,
        "url": true,
        "event": true,
        "require": true,
        /*dom,bom*/
        "FormData": true,
        "XMLHttpRequest": true,
        "window": true,
        "document": true,
        "console": true,
        "alert": true,
        "localStorage": true,
        "sessionStorage": true,
        "setTimeout": true,
        "setInterval": true
    },
    "rules": {
        "no-redeclare": 1,
        "no-console": 1,
        "no-undef": 1,
        "no-unused-vars": 1
    }
}
